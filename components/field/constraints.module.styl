.middleBox
  top 32px
  right 32px
  bottom 32px
  left 32px
  z-index -1
.check
  border-color #fff
.notAllowCheck
  cursor not-allowed
.leftSolid
  border-left-style: solid
.topSolid
  border-top-style: solid
