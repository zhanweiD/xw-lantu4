.topLine
  top 0
  height 30px
  width 1px
  border-left 1px dashed rgba(255, 255, 255, .1)

.rightLine
  right 0
  height 1px
  width 30px
  border-top 1px dashed rgba(255, 255, 255, .1)

.bottomLine
  bottom 0
  height 30px
  width 1px
  border-left 1px dashed rgba(255, 255, 255, .1)

.leftLine
  left 0
  height 1px
  width 30px
  border-top 1px dashed rgba(255, 255, 255, .1)

.widthLine
  left 1px
  height 1px
  right 1px
  border-top 1px dashed rgba(255, 255, 255, .1)

.heightLine
  top 1px
  bottom 1px
  width 1px
  border-left 1px dashed rgba(255, 255, 255, .1)

.topDiv
  top 0
  height 32px
  width 10px

.rightDiv
  right 0
  height 10px
  width 32px

.bottomDiv
  bottom 0
  height 32px
  width 10px

.leftDiv
  left 0
  height 10px
  width 32px

.widthDiv
  left 31px
  height 10px
  width calc(100% - 64px)

.heightDiv
  top 31px
  height calc(100% - 64px)
  width 10px

