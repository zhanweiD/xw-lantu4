.box
  border 1px dashed rgba(255, 255, 255, 0.1)

.leftArrow1
  display: inline-block
  left: 0
  border-left: 1px solid
  border-bottom: 1px solid
  width: 5px
  height: 5px
  border-color: rgba(255, 255, 255, .1)
  transform: rotate(45deg)

.leftArrow2
  display: inline-block
  right: 2px
  border-left: 1px solid
  border-bottom: 1px solid
  width: 5px
  height: 5px
  border-color: rgba(255, 255, 255, .1)
  transform: rotate(-135deg)

